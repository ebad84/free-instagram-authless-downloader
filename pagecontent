<script>
    function getEmail() {
        let data = $('.get-email-form').serialize();
        $.post(folder + "/app/controllers/ajax.php", data, (data) => {
            $('.get-email-form').html(data);
        });
    }
    window.passbackCallback = function(divid) {
        document.getElementById(divid).parentElement.remove();
    }
</script>

